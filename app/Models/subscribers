module.exports = function(sequelize , Sequelize){
    const options = {
        timestamps: false
    };
    const Sub = sequelize.define('subscribers', {

        id: {
            type: Sequelize.INTEGER,
            primaryKey: true,
            autoIncrement: true
        },

        user_id: {
            type: Sequelize.INTEGER,
            notEmpty: true,
        },

        sub_id: {
            type: Sequelize.INTEGER,
            notEmpty: true,
        },

        created_at: Sequelize.DATE,

        updated_at: Sequelize.DATE,

    }, options);

    return Sub;
};